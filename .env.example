# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/mangonoteweb

# Vercel Blob Storage (for file uploads)
BLOB_READ_WRITE_TOKEN=your_vercel_blob_token_here

# OpenRouter AI Service (Primary)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# YouTube Transcript Service (SupaData API)
SUPADATA_API_KEY=your_supadata_api_key_here

# Optional: Direct API keys for fallback (if needed)
OPENAI_API_KEY=your_openai_key_fallback
ANTHROPIC_API_KEY=your_anthropic_key_fallback

# NextAuth Configuration
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development